  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-modal",
          "react-native-snap-carousel",
          "react-native-switch-selector",
          "@apollo/client",
          "@expo-google-fonts/inter",
          "@expo-google-fonts/noto-sans-jp",
          "@expo-google-fonts/roboto-condensed",
          "@react-native-community/masked-view",
          "compare-versions",
          "firebase",
          "graphql",
          "graphql-anywhere",
          "graphql-tag",
          "react-native-app-link",
          "uuid",
          "@graphql-typed-document-node/core",
          "cross-fetch",
          "dotenv",
          "@react-native/metro-config",
          "@sentry/cli"
        ]
      }
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry/cli"
    ]
  }
