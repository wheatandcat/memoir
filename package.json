    "@types/babel__core": "~7.1.15",
    "@types/enzyme": "^3.10.9",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^26.0.24",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@types/react-native": "^0.69.5",
    "@types/react-native-app-link": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "graphql-codegen-typescript-mock-data": "^2.2.5",
    "husky": "^7.0.0",
    "hygen": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-expo": "^46.0.0",
    "jest-expo-enzyme": "^1.2.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.0.0",
    "loki": "^0.28.1",
    "mockdate": "^3.0.5",
    "msw": "^0.45.0",
    "react-native-storybook-loader": "^2.0.4",
    "react-test-renderer": "^18.2.0",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-publish-gcs-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "ts-jest": "^27.0.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^4.6.3",
    "typesync": "^0.8.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.*",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn eslint --fix"
    ]
  },
  "resolutions": {
    "@types/react": "^18.0.8",
    "**/@types/react": "^18.0.8"
  }
}
