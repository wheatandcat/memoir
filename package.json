{
  "name": "memoir",
  "version": "1.8.0",
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "expo:start": "APP_ENV=review expo start -i",
    "init:app:dev": "sh scripts/local/setup.sh",
    "init:app:review": "sh scripts/review/setup.sh",
    "init:app:production": "sh scripts/production/setup.sh",
    "init:storybook": "sh scripts/storybook/setup.sh",
    "android": "yarn init:app:dev && yarn expo:start --android",
    "ios": "yarn init:app:dev && yarn expo:start --ios",
    "web": "yarn init:app:dev && yarn expo:start --web",
    "ios:clear": "yarn init:app:dev && yarn expo:start --ios",
    "android:clear": "yarn init:app:dev && yarn expo:start --android -c",
    "ios:build": "yarn init:app:production && eas build --profile=production -p=ios",
    "ios:build:dev": "yarn init:app:production && eas build --profile=development -p=ios",
    "android:build": "yarn init:app:production && eas build --profile=production -p=android",
    "android:build:dev": "yarn init:app:production && eas build --profile=development -p=android",
    "store:upload": "yarn init:app:production && eas build --profile=production --platform all --non-interactive --auto-submit --no-wait",
    "loki:test": "yarn loki test ; sh scripts/storybook/setup.sh",
    "loki:update": "rm -rf .loki/reference/*.png && yarn loki update ; sh scripts/storybook/setup.sh",
    "start-server": "react-native-storybook-server",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "storybook": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start --android",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**'",
    "download:schema.graphqls": "curl -L -O https://raw.githubusercontent.com/wheatandcat/memoir-backend/main/graph/schema.graphqls",
    "codegen": "yarn download:schema.graphqls &  TZ=Asia/Tokyo graphql-codegen",
    "codegen:lint:fix": "eslint --fix ./src/queries/api/index.ts",
    "test": "jest",
    "prepare": "husky install",
    "unused-exports": "node ./scripts/ts-unused-exports",
    "compare-images": "reg-suit sync-expected && reg-suit compare"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/noto-sans-jp": "^0.2.0",
    "@expo-google-fonts/roboto-condensed": "^0.2.0",
    "@expo/react-native-action-sheet": "^3.13.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@types/compare-versions": "^3.3.0",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/uuid": "^8.3.4",
    "compare-versions": "^5.0.1",
    "dayjs": "^1.11.5",
    "expo": "^52.0.23",
    "expo-apple-authentication": "~7.1.2",
    "expo-application": "~6.0.1",
    "expo-auth-session": "~6.0.1",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-device": "~7.0.1",
    "expo-file-system": "~18.0.6",
    "expo-font": "~13.0.2",
    "expo-gl": "~15.0.2",
    "expo-image": "~2.0.3",
    "expo-image-manipulator": "~13.0.5",
    "expo-image-picker": "~16.0.3",
    "expo-intent-launcher": "~12.0.1",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.3",
    "expo-notifications": "~0.29.11",
    "expo-router": "~4.0.15",
    "expo-sharing": "~13.0.0",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "firebase": "^9.12.1",
    "graphql": "^16.6.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-app-link": "^1.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~3.16.1",
    "react-native-root-toast": "^3.4.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-snap-carousel": "https://github.com/LovebugLLC/react-native-snap-carousel.git",
    "react-native-switch-selector": "2.1.4",
    "react-native-view-shot": "~4.0.3",
    "react-native-webview": "13.12.5",
    "recoil": "^0.7.6",
    "sentry-expo": "~7.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/config": "^10.0.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/gql-tag-operations-preset": "^1.6.0",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typed-document-node": "^2.3.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-eslint/eslint-plugin": "^1.1.2",
    "@react-native-community/eslint-config": "^3.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-controls": "^6.5.14",
    "@storybook/addon-ondevice-actions": "^6.5.1",
    "@storybook/addon-ondevice-controls": "^6.5.1",
    "@storybook/react-native": "^6.5.1",
    "@storybook/react-native-server": "^6.5.4",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.0.0",
    "@types/babel__core": "~7.1.15",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^26.0.24",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "@types/react-native-app-link": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "graphql-codegen-typescript-mock-data": "^2.2.5",
    "husky": "^8.0.1",
    "hygen": "^6.2.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.2",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.0.3",
    "loki": "^0.30.3",
    "mockdate": "^3.0.5",
    "msw": "^0.45.0",
    "react-test-renderer": "^18.2.0",
    "reg-keygen-git-hash-plugin": "^0.12.1",
    "reg-notify-github-plugin": "^0.12.1",
    "reg-publish-gcs-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "ts-jest": "^27.0.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^5.3.3",
    "typesync": "^0.9.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn eslint --fix"
    ]
  },
  "resolutions": {
    "@types/react": "^18.0.8",
    "**/@types/react": "^18.0.8"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-modal",
          "react-native-snap-carousel",
          "react-native-switch-selector",
          "@apollo/client",
          "@expo-google-fonts/inter",
          "@expo-google-fonts/noto-sans-jp",
          "@expo-google-fonts/roboto-condensed",
          "@react-native-community/masked-view",
          "compare-versions",
          "firebase",
          "graphql",
          "graphql-anywhere",
          "graphql-tag",
          "react-native-app-link",
          "recoil",
          "sentry-expo",
          "uuid"
        ]
      }
    }
  }
}
