{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "init:app": "cp -r App.app.tsx App.tsx",
    "init:storybook": "cp -r App.storybook.tsx App.tsx && sh scripts/storybook/setup.sh",
    "start": "yarn init:app && expo start",
    "android": "yarn init:app && expo start --android",
    "ios": "yarn init:app && expo start --ios",
    "web": "yarn init:app && expo start --web",
    "ios:clear": "yarn init:app && expo start --ios -c",
    "android:clear": "yarn init:app && expo start --android -c",
    "loki:test": "sh scripts/visual-testing/setup.sh && yarn loki test ; sh scripts/storybook/setup.sh",
    "loki:update": "sh scripts/visual-testing/setup.sh && rm -rf .loki/reference/*.png && yarn loki update ; sh scripts/storybook/setup.sh",
    "storybook-server": "(adb reverse tcp:7007 tcp:7007 > /dev/null || true) && echo 'y' | start-storybook -p 7007 | react-native start --projectRoot storybook --watchFolders $PWD",
    "storybook": "yarn init:storybook && yarn storybook-server",
    "storybook:ios": "yarn init:storybook && expo start --ios -c",
    "storybook:android": "yarn init:storybook && expo start --android -c",
    "prestorybook": "rnstl",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**'",
    "download:schema.graphqls": "curl -L -O https://raw.githubusercontent.com/wheatandcat/memoir-backend/main/graph/schema.graphqls",
    "codegen": "yarn download:schema.graphqls & graphql-codegen",
    "codegen:lint:fix": "eslint --fix ./src/queries/api/index.ts",
    "test": "jest",
    "prepare": "husky install",
    "unused-exports": "node ./scripts/ts-unused-exports",
    "compare-images": "reg-suit sync-expected && reg-suit compare"
  },
  "dependencies": {
    "@apollo/client": "3.3.21",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/noto-sans-jp": "^0.2.0",
    "@expo-google-fonts/roboto-condensed": "^0.2.0",
    "@expo/react-native-action-sheet": "^3.9.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "1.16.3",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@types/react-native-snap-carousel": "^3.8.4",
    "@types/uuid": "^8.3.1",
    "dayjs": "^1.10.6",
    "expo": "^42.0.0",
    "expo-app-loading": "^1.1.2",
    "expo-apple-authentication": "~3.2.1",
    "expo-application": "~3.2.0",
    "expo-auth-session": "~3.3.1",
    "expo-clipboard": "~1.1.0",
    "expo-constants": "~11.0.1",
    "expo-crypto": "~9.2.0",
    "expo-device": "~3.3.0",
    "expo-file-system": "~11.1.3",
    "expo-font": "~9.2.1",
    "expo-image-manipulator": "~9.2.2",
    "expo-image-picker": "~10.2.2",
    "expo-intent-launcher": "~9.1.0",
    "expo-linear-gradient": "~9.2.0",
    "expo-linking": "~2.3.1",
    "expo-notifications": "~0.12.3",
    "expo-random": "~11.2.0",
    "expo-sharing": "~9.2.1",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "expo-web-browser": "~9.2.0",
    "firebase": "8.7.1",
    "graphql": "^15.5.1",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-get-random-values": "~1.7.0",
    "react-native-modal": "^12.0.2",
    "react-native-modal-datetime-picker": "^10.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.2.0",
    "react-native-root-toast": "^3.2.1",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-swipe-gestures": "git+https://git@github.com/EatherToo/react-native-swipe-gestures",
    "react-native-switch-selector": "^2.1.4",
    "react-native-view-shot": "3.1.2",
    "react-native-web": "~0.17.1",
    "recoil": "^0.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@expo/config": "^5.0.3",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-react-apollo": "^2.3.0",
    "@graphql-eslint/eslint-plugin": "^1.1.2",
    "@react-native-community/eslint-config": "^3.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@storybook/addon-actions": "^6.1",
    "@storybook/addon-knobs": "^6.1",
    "@storybook/addon-links": "^6.1",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.0",
    "hygen": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-expo": "^42.0.0",
    "jest-expo-enzyme": "^1.2.1",
    "lint-staged": "^11.0.0",
    "loki": "^0.28.1",
    "react-native-storybook-loader": "^2.0.4",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-publish-gcs-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "ts-jest": "^27.0.3",
    "ts-unused-exports": "^7.0.3",
    "typescript": "~4.3.5"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.*",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn eslint --fix"
    ]
  }
}
