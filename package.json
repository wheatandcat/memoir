{
  "name": "memoir",
  "version": "1.9.0",
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "expo:start": "APP_ENV=review expo start",
    "expo:start:clear": "APP_ENV=review expo start --clear",
    "init:app:dev": "sh scripts/local/setup.sh",
    "init:app:review": "sh scripts/review/setup.sh",
    "init:app:production": "sh scripts/production/setup.sh",
    "android": "pnpm init:app:dev && pnpm expo:start --android",
    "ios": "pnpm init:app:dev && pnpm expo:start --ios",
    "web": "pnpm init:app:dev && pnpm expo:start --web",
    "ios:clear": "pnpm init:app:dev && pnpm expo:start:clear --ios",
    "dev:ios": "pnpm init:app:dev && npx expo run:ios",
    "dev:ios:production": "pnpm init:app:production && npx expo run:ios",
    "dev:android": "pnpm init:app:dev && npx expo run:android",
    "dev:android:production": "pnpm init:app:production && npx expo run:android",
    "android:clear": "pnpm init:app:dev && pnpm expo:start --android -c",
    "ios:build": "pnpm init:app:production && eas build --profile=production -p=ios",
    "ios:build:local": "pnpm init:app:production && eas build --profile=production -p=ios --local --clear-cache",
    "ios:build:dev": "pnpm init:app:production && eas build --profile=development-simulator -p=ios",
    "android:build": "pnpm init:app:production && eas build --profile=production -p=android",
    "android:build:local": "pnpm init:app:production && eas build --profile=production -p=android --local",
    "android:build:dev": "pnpm init:app:production && eas build --profile=preview -p=android",
    "store:upload": "pnpm init:app:production && eas build --profile=production --platform all --non-interactive --auto-submit --no-wait",
    "format": "npx @biomejs/biome check --write ./*",
    "download:schema.graphqls": "curl -L --retry 3 --retry-delay 5 --connect-timeout 10 --max-time 30 -O https://raw.githubusercontent.com/wheatandcat/memoir-backend/main/graph/schema.graphqls",
    "codegen": "pnpm download:schema.graphqls && TZ=Asia/Tokyo graphql-codegen",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:ci": "biome ci",
    "prepare": "if [ -d .git ] && [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "unused-exports": "node ./scripts/ts-unused-exports",
    "compare-images": "reg-suit sync-expected && reg-suit compare",
    "xcode": "open ios/memoir.xcworkspace",
    "storybook:ios": "EXPO_PUBLIC_STORYBOOK_ENABLED=true npx expo run:ios",
    "storybook:android": "EXPO_PUBLIC_STORYBOOK_ENABLED=true npx expo run:android",
    "storybook-generate": "sb-rn-get-stories"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/noto-sans-jp": "^0.2.0",
    "@expo-google-fonts/roboto-condensed": "^0.2.0",
    "@expo/react-native-action-sheet": "^3.13.0",
    "@expo/vector-icons": "^14.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-picker/picker": "2.11.0",
    "@react-native/metro-config": "^0.79.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
