{
  "name": "memoir",
  "version": "1.8.0",
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "expo:start": "APP_ENV=review expo start",
    "init:app:dev": "sh scripts/local/setup.sh",
    "init:app:review": "sh scripts/review/setup.sh",
    "init:app:production": "sh scripts/production/setup.sh",
    "init:storybook": "sh scripts/storybook/setup.sh",
    "android": "pnpm init:app:dev && pnpm expo:start --android",
    "ios": "pnpm init:app:dev && pnpm expo:start --ios",
    "web": "pnpm init:app:dev && pnpm expo:start --web",
    "ios:clear": "pnpm init:app:dev && pnpm expo:start --ios",
    "android:clear": "pnpm init:app:dev && pnpm expo:start --android -c",
    "ios:build": "pnpm init:app:production && eas build --profile=production -p=ios",
    "ios:build:local": "pnpm init:app:production && eas build --profile=production -p=ios --local",
    "ios:build:dev": "pnpm init:app:production && eas build --profile=development-simulator -p=ios",
    "android:build": "pnpm init:app:production && eas build --profile=production -p=android",
    "android:build:local": "pnpm init:app:production && eas build --profile=production -p=android --local",
    "android:build:dev": "pnpm init:app:production && eas build --profile=preview -p=android",
    "store:upload": "pnpm init:app:production && eas build --profile=production --platform all --non-interactive --auto-submit --no-wait",
    "loki:test": "pnpm loki test ; sh scripts/storybook/setup.sh",
    "loki:update": "rm -rf .loki/reference/*.png && pnpm loki update ; sh scripts/storybook/setup.sh",
    "start-server": "react-native-storybook-server",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "storybook": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start --android",
    "format": "npx @biomejs/biome check --write ./*",
    "download:schema.graphqls": "curl -L --retry 3 --retry-delay 5 --connect-timeout 10 --max-time 30 -O https://raw.githubusercontent.com/wheatandcat/memoir-backend/main/graph/schema.graphqls",
    "codegen": "pnpm download:schema.graphqls && TZ=Asia/Tokyo graphql-codegen",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:ci": "biome ci",
    "prepare": "if [ -d .git ] && [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "unused-exports": "node ./scripts/ts-unused-exports",
    "compare-images": "reg-suit sync-expected && reg-suit compare"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/noto-sans-jp": "^0.2.0",
    "@expo-google-fonts/roboto-condensed": "^0.2.0",
    "@expo/react-native-action-sheet": "^3.13.0",
    "@expo/vector-icons": "^14.0.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@sentry/cli": "^2.43.0",
    "@sentry/react-native": "~6.3.0",
    "@types/compare-versions": "^3.3.0",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/uuid": "^8.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "compare-versions": "^5.0.1",
    "cross-fetch": "^4.1.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.4.7",
    "expo": "~52.0.38",
    "expo-apple-authentication": "~7.1.3",
    "expo-application": "~6.0.2",
    "expo-asset": "~11.0.3",
    "expo-auth-session": "~6.0.2",
    "expo-blur": "~14.0.3",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.14",
    "expo-device": "~7.0.2",
    "expo-file-system": "~18.0.8",
    "expo-font": "~13.0.3",
    "expo-gl": "~15.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.4",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.4",
    "expo-intent-launcher": "~12.0.2",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-modules-autolinking": "*",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.1",
    "expo-system-ui": "~4.0.7",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "firebase": "^9.12.1",
    "graphql": "^16.10.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-app-link": "^1.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-root-toast": "^3.4.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-snap-carousel": "https://github.com/LovebugLLC/react-native-snap-carousel.git",
    "react-native-switch-selector": "2.1.4",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "recoil": "^0.7.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.26.7",
    "@biomejs/biome": "1.9.4",
    "@expo/config": "^10.0.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/gql-tag-operations-preset": "^1.6.0",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typed-document-node": "^2.3.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-controls": "^6.5.14",
    "@storybook/addon-ondevice-actions": "^6.5.1",
    "@storybook/addon-ondevice-controls": "^6.5.1",
    "@storybook/react-native": "^6.5.1",
    "@storybook/react-native-server": "^6.5.4",
    "@testing-library/react-native": "^11.5.4",
    "@types/babel__core": "~7.1.15",
    "@types/jest": "^26.0.24",
    "@types/node": "^22.10.10",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "@types/react-native-app-link": "^1.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-preset-expo": "^12.0.6",
    "cross-env": "^7.0.3",
    "graphql-codegen-typescript-mock-data": "^2.2.5",
    "husky": "^8.0.1",
    "hygen": "^6.2.11",
    "identity-obj-proxy": "^3.0.0",
    "jest-expo": "~52.0.6",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.0.3",
    "loki": "^0.30.3",
    "mockdate": "^3.0.5",
    "msw": "^2.7.0",
    "react-test-renderer": "^18.2.0",
    "reg-keygen-git-hash-plugin": "^0.12.1",
    "reg-notify-github-plugin": "^0.12.1",
    "reg-publish-gcs-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^5.3.3",
    "typesync": "^0.9.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-modal",
          "react-native-snap-carousel",
          "react-native-switch-selector",
          "@apollo/client",
          "@expo-google-fonts/inter",
          "@expo-google-fonts/noto-sans-jp",
          "@expo-google-fonts/roboto-condensed",
          "@react-native-community/masked-view",
          "compare-versions",
          "firebase",
          "graphql",
          "graphql-anywhere",
          "graphql-tag",
          "react-native-app-link",
          "recoil",
          "uuid",
          "@graphql-typed-document-node/core",
          "cross-fetch",
          "dotenv"
        ]
      }
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry/cli"
    ]
  }
}
