{
  "name": "memoir",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "expo:start": "APP_ENV=review expo start",
    "init:app:dev": "sh scripts/local/setup.sh",
    "init:app:review": "sh scripts/review/setup.sh",
    "init:app:production": "sh scripts/production/setup.sh",
    "init:storybook": "sh scripts/storybook/setup.sh",
    "start": "yarn init:app:dev && yarn expo:start",
    "android": "yarn init:app:dev && yarn expo:start --android",
    "ios": "yarn init:app:dev && yarn expo:start --ios",
    "web": "yarn init:app:dev && yarn expo:start --web",
    "ios:clear": "yarn init:app:dev && yarn expo:start --ios",
    "android:clear": "yarn init:app:dev && yarn expo:start --android -c",
    "ios:build": "yarn init:app:production && eas build --profile=production -p=ios",
    "ios:build:dev": "yarn init:app:production && eas build --profile=development -p=ios",
    "android:build": "yarn init:app:production && eas build --profile=production -p=android",
    "android:build:dev": "yarn init:app:production && eas build --profile=development -p=android",
    "store:upload": "yarn init:app:production && npx eas build --profile=production  --platform all --non-interactive --auto-submit --no-wait",
    "loki:test": "sh scripts/visual-testing/setup.sh && yarn loki test ; sh scripts/storybook/setup.sh",
    "loki:update": "sh scripts/visual-testing/setup.sh && rm -rf .loki/reference/*.png && yarn loki update ; sh scripts/storybook/setup.sh",
    "storybook-server": "(adb reverse tcp:7007 tcp:7007 > /dev/null || true) && echo 'y' | start-storybook -p 7007 | react-native start --projectRoot storybook --watchFolders $PWD",
    "storybook": "yarn init:storybook && yarn storybook-server",
    "storybook:ios": "yarn init:storybook && yarn expo:start --ios",
    "storybook:android": "yarn init:storybook && yarn expo:start --android",
    "prestorybook": "rnstl",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**'",
    "download:schema.graphqls": "curl -L -O https://raw.githubusercontent.com/wheatandcat/memoir-backend/main/graph/schema.graphqls",
    "codegen": "yarn download:schema.graphqls & graphql-codegen",
    "codegen:lint:fix": "eslint --fix ./src/queries/api/index.ts",
    "test": "jest",
    "prepare": "husky install",
    "unused-exports": "node ./scripts/ts-unused-exports",
    "compare-images": "reg-suit sync-expected && reg-suit compare"
  },
  "dependencies": {
    "@apollo/client": "3.3.21",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/noto-sans-jp": "^0.2.0",
    "@expo-google-fonts/roboto-condensed": "^0.2.0",
    "@expo/react-native-action-sheet": "^3.13.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.4.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@types/compare-versions": "^3.3.0",
    "@types/react-native-snap-carousel": "^3.8.4",
    "@types/uuid": "^8.3.1",
    "compare-versions": "^3.6.0",
    "dayjs": "^1.10.6",
    "expo": "^45.0.0",
    "expo-apple-authentication": "~4.2.1",
    "expo-application": "~4.1.0",
    "expo-auth-session": "~3.6.1",
    "expo-clipboard": "~3.0.1",
    "expo-constants": "~13.1.1",
    "expo-crypto": "~10.2.0",
    "expo-dev-client": "~1.0.0",
    "expo-device": "~4.2.0",
    "expo-file-system": "~14.0.0",
    "expo-font": "~10.1.0",
    "expo-gl": "~11.3.0",
    "expo-image-manipulator": "~10.3.1",
    "expo-image-picker": "~13.1.1",
    "expo-intent-launcher": "~10.2.0",
    "expo-linear-gradient": "~11.3.0",
    "expo-linking": "~3.1.0",
    "expo-notifications": "~0.15.2",
    "expo-random": "~12.2.0",
    "expo-sharing": "~10.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.2",
    "expo-web-browser": "~10.2.1",
    "firebase": "^9.8.2",
    "graphql": "^15.5.1",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-app-link": "^1.0.1",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-get-random-values": "~1.8.0",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^10.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.8.0",
    "react-native-root-toast": "^3.4.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-snap-carousel": "https://github.com/LovebugLLC/react-native-snap-carousel.git",
    "react-native-swipe-gestures": "git+https://git@github.com/EatherToo/react-native-swipe-gestures",
    "react-native-switch-selector": "^2.1.4",
    "react-native-view-shot": "3.1.2",
    "react-native-webview": "11.18.1",
    "recoil": "^0.3.1",
    "sentry-expo": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/config": "^6.0.24",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-react-apollo": "^2.3.0",
    "@graphql-eslint/eslint-plugin": "^1.1.2",
    "@react-native-community/eslint-config": "^3.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@storybook/addon-actions": "^6.1",
    "@storybook/addon-knobs": "^6.1",
    "@storybook/addon-links": "^6.1",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@types/babel__core": "~7.1.15",
    "@types/enzyme": "^3.10.9",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^26.0.24",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.11",
    "@types/react-native": "~0.67.6",
    "@types/react-native-app-link": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.0",
    "hygen": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-expo": "^45.0.0",
    "jest-expo-enzyme": "^1.2.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.0.0",
    "loki": "^0.28.1",
    "mockdate": "^3.0.5",
    "react-native-storybook-loader": "^2.0.4",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-publish-gcs-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "ts-jest": "^27.0.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "~4.3.5",
    "typesync": "^0.8.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.*",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn eslint --fix"
    ]
  }
}
