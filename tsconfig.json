{
  "compilerOptions": {
    "types": [
      "jest",
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "config/*": [
        "src/config/*"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "storyBookUtils/*": [
        "src/storyBookUtils/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "containers/*": [
        "src/containers/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "queries/*": [
        "src/queries/*"
      ],
      "__mockData__/*": [
        "src/__mockData__/*"
      ],
      "mocks/*": [
        "src/mocks/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "scripts/send-metrics"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
